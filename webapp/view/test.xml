			<Column id="today_6" minScreenWidth="Tablet" demandPopin="true"><Text text="{dates>/dates/today_6}" /></Column>
		</columns>
		<items>
			<ColumnListItem
				type="Active"
				press="onPress">
				<cells>
					<Text text="{name}" />
