			<Column id="productCol" width="12em"><Text text="Center" /></Column>
			<Column id="today" minScreenWidth="Tablet" demandPopin="true"><Text text="{dates>/dates/today}"/></Column>
			<Column id="today_1" minScreenWidth="Tablet" demandPopin="true"><Text text="{dates>/dates/today_1}" /></Column>
			<Column id="today_2" minScreenWidth="Tablet" demandPopin="true"><Text text="{dates>/dates/today_2}" /></Column>
			<Column id="today_3" minScreenWidth="Tablet" demandPopin="true"><Text text="{dates>/dates/today_3}" /></Column>
			<Column id="today_4" minScreenWidth="Tablet" demandPopin="true"><Text text="{dates>/dates/today_4}" /></Column>
